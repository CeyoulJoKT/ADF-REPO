{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          ProductID as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 derive(ProductID = md5(ProductID)) ~> derivedColumn1",
				"derivedColumn1 output(mapColumn(",
				"          ProductID",
				"     )) ~> output1"
			]
		}
	}
}